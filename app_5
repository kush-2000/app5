<button onclick="myFunction()">Total Number of Nucleotides</button>


<p id="DIY"></p>

<script>

  var dna = "";
  var n = dna.length;
  function myFunction() {
  document.getElementById("DIY").innerHTML = n;
}
  
</script>







<br><br><br>

<button onclick="aSequence()">Total Number of 'A' Nucleotides</button>

<p id="anucleus"></p>

<script>
  var match1 = dna.match(/A/g).length;
  function aSequence() {
  document.getElementById("anucleus").innerHTML = match1;
}
</script>

<button onclick="aPer()">Percentage of 'A' Base</button>
<p id="aBase"></p>
<script>
  var per1 = (match1/n)*100;
  function aPer() {
  document.getElementById("aBase").innerHTML = per1 +'%';
}
</script>




<br><br><br>

